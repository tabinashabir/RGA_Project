package com.lnghealthriskcare.domain;

public class MedicalDisclosureEvent {
   private String underwritingSTP = "";
   private String asthmaSeverity = "";
   private String reasons = "";
   private double debits;
   private String requirements = "";
   private String attackSeverity = "";
   private String oralSteroidRisk = "";
   private String planOffer = "";
   private int riskScore = 0;
   private String underwritingHandlingType = "";
   private String medicalDisclosureArea = "";
   private int timeSinceDiagnosis;
   private String suplementalOxygenUse;
   private String refferedToAsStatusAsthmaticus = "";
   private String limitedActivitesOrTimeOffworkinLast6Months = "";
   private String hospitalizedInTheLast5Years = "";
   private String requireOralSteroids = "";
   private int oralSteroidFrequency;
   private int attacksPerWeek;
   
   public String getRefferedToAsStatusAsthmaticus() {
		return refferedToAsStatusAsthmaticus;
	}
	public void setRefferedToAsStatusAsthmaticus(String refferedToAsStatusAsthmaticus) {
		this.refferedToAsStatusAsthmaticus = refferedToAsStatusAsthmaticus;
	}
	public String getLimitedActivitesOrTimeOffworkinLast6Months() {
		return limitedActivitesOrTimeOffworkinLast6Months;
	}
	public void setLimitedActivitesOrTimeOffworkinLast6Months(String limitedActivitesOrTimeOffworkinLast6Months) {
		this.limitedActivitesOrTimeOffworkinLast6Months = limitedActivitesOrTimeOffworkinLast6Months;
	}
	public String getHospitalizedInTheLast5Years() {
		return hospitalizedInTheLast5Years;
	}
	public void setHospitalizedInTheLast5Years(String hospitalizedInTheLast5Years) {
		this.hospitalizedInTheLast5Years = hospitalizedInTheLast5Years;
	}
	public String getRequireOralSteroids() {
		return requireOralSteroids;
	}
	public void setRequireOralSteroids(String requireOralSteroids) {
		this.requireOralSteroids = requireOralSteroids;
	}
	public int getOralSteroidFrequency() {
		return oralSteroidFrequency;
	}
	public void setOralSteroidFrequency(int oralSteroidFrequency) {
		this.oralSteroidFrequency = oralSteroidFrequency;
	}
	public int getAttacksPerWeek() {
		return attacksPerWeek;
	}
	public void setAttacksPerWeek(int attacksPerWeek) {
		this.attacksPerWeek = attacksPerWeek;
	}
	public String getMedicalExamResults() {
		return medicalExamResults;
	}
	public void setMedicalExamResults(String medicalExamResults) {
		this.medicalExamResults = medicalExamResults;
	}
	private String medicalExamResults;
   
	public String getSuplementalOxygenUse() {
		return suplementalOxygenUse;
	}
	public void setSuplementalOxygenUse(String suplementalOxygenUse) {
		this.suplementalOxygenUse = suplementalOxygenUse;
	}
	public String getUnderwritingSTP() {
		return underwritingSTP;
	}
	public void setUnderwritingSTP(String underwritingSTP) {
		this.underwritingSTP = underwritingSTP;
	}
	public String getAsthmaSeverity() {
		return asthmaSeverity;
	}
	public void setAsthmaSeverity(String asthmaSeverity) {
		this.asthmaSeverity = asthmaSeverity;
	}
	public String getReasons() {
		return reasons;
	}
	public void setReasons(String reasons) {
		this.reasons = reasons;
	}
	public double getDebits() {
		return debits;
	}
	public void setDebits(double debits) {
		this.debits = debits;
	}
	public String getRequirements() {
		return requirements;
	}
	public void setRequirements(String requirements) {
		this.requirements = requirements;
	}
	public String getAttackSeverity() {
		return attackSeverity;
	}
	public void setAttackSeverity(String attackSeverity) {
		this.attackSeverity = attackSeverity;
	}
	public String getOralSteroidRisk() {
		return oralSteroidRisk;
	}
	public void setOralSteroidRisk(String oralSteroidRisk) {
		this.oralSteroidRisk = oralSteroidRisk;
	}
	public String getPlanOffer() {
		return planOffer;
	}
	public void setPlanOffer(String planOffer) {
		this.planOffer = planOffer;
	}
	public int getRiskScore() {
		return riskScore;
	}
	public void setRiskScore(int riskScore) {
		this.riskScore = riskScore;
	}
	public String getUnderwritingHandlingType() {
		return underwritingHandlingType;
	}
	public void setUnderwritingHandlingType(String underwritingHandlingType) {
		this.underwritingHandlingType = underwritingHandlingType;
	}
	public String getMedicalDisclosureArea() {
		return medicalDisclosureArea;
	}
	public void setMedicalDisclosureArea(String medicalDisclosureArea) {
		this.medicalDisclosureArea = medicalDisclosureArea;
	}
	public int getTimeSinceDiagnosis() {
		return timeSinceDiagnosis;
	}
	public void setTimeSinceDiagnosis(int timeSinceDiagnosis) {
		this.timeSinceDiagnosis = timeSinceDiagnosis;
	}

	
	
}
